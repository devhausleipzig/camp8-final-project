generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    Int    @id @default(autoincrement())
  name  String
  email String
  lists List[]
}

model List {
  id         Int          @id @default(autoincrement())
  items      User         @relation(fields: [itemsId], references: [id])
  itemsId    Int
  Items      Items[]
  Categories Categories[]
}

model Items {
  id         Int  @id @default(autoincrement())
  Category   List @relation(fields: [CategoryID], references: [id])
  CategoryID Int
}

model Categories {
  id     Int  @id @default(autoincrement())
  name   List @relation(fields: [NameID], references: [id])
  NameID Int
}
