generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  indentifier String @id @unique @default(uuid())
  name        String
  email       String
  saltAndHash String
}

model Lists {
  indentifier String @id @unique @default(uuid())
}
